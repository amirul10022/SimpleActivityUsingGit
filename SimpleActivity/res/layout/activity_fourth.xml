<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <TextView
        android:id="@+id/textViewFourthActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/hello_world" />
    
    
     
    <TextView 
        android:id="@+id/textViewFifthActivityId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewFourthActivity"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="50dp"
        android:text="@string/text_view_fourth_activity_layout"
        />
    
    <Button 
        android:id="@+id/buttonSecondActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:onClick="onClick"
        android:layout_below="@+id/textViewFifthActivityId"
        android:text="@string/button_text_fifth_activity"/>
    

</RelativeLayout>
